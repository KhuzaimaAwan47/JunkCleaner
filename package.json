{
  "name": "junkcleaner",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "eas-prod-android": "eas build --profile production --platform android",
    "prebuild-android": "expo prebuild --platform android",
    "prebuild-ios": "expo prebuild --platform ios",
    "assemble-release": "expo prebuild --clean && cd android && ./gradlew clean && ./gradlew assembleRelease --stacktrace",
    "bundle-release-mac": "expo prebuild --clean && bash signing-config.sh && cd android && ./gradlew clean && ./gradlew bundleRelease --stacktrace",
    "bundle-release-windows": "expo prebuild --clean && powershell -ExecutionPolicy Bypass -File signing-config.ps1 && cd android && ./gradlew clean && ./gradlew bundleRelease --stacktrace"
  },
  "dependencies": {
    "@emotion/unitless": "0.7.5",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "bloom-filters": "^3.0.4",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.23",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.0.2",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "moti": "^0.29.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~19.1.0",
    "@types/styled-components": "^5.1.35",
    "@types/styled-components-react-native": "^5.2.5",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "resolutions": {
    "@emotion/unitless": "0.7.5"
  },
  "private": true
}
